<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.avfweb.appbase" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>AVF Appbase</name>
    <description>
        App base com acesso a banco de dados local e acesso a webservice
    </description>
    <author email="viniciusblink@gmail.com" href="http://avfweb.com.br">
        Vinicius Fernandes
    </author>
    <content src="index.html" />
    <icon src="www/img/logoavf.png" />
    <splash src="www/img/logoavf.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="Fullscreen" value="false" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="FadeSplashScreenDuration" value="800" />
</widget>
